{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Logstash": {
      "Url": "http://logstash:5044",
      "Enabled": true
    }
  },
  "AllowedHosts": "*",
  "GrpcSettings": {
    "DomainServiceUrl": "http://grpc-service:5085"
  },
  "Redis": {
    "ConnectionString": "redis:6379"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "gRPC": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "AutoRabbitMQ": "amqp://guest:guest@rabbitmq:5672"
  }
}
