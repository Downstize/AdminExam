{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GrpcSettings": {
    "DomainServiceUrl": "http://grpc-service:5085"
  },
  "Redis": {
    "ConnectionString": "redis:6379"
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "gRPC": {
      "Protocols": "Http2"
    }
  }
}
